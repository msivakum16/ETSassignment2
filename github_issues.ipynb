      "source": [
        "from github import Github\n",
        "import collections\n",
        "import json\n",
        "import os\n",
        "import signal\n",
        "import sys\n",
        "import subprocess\n",
        "import pandas as pd"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "# **Script to access the repository : https://github.com/rails/rails/issues and get the last 500 issue data**\n",
        "\n",
        "Reference:\n",
        "\n",
        "https://github.com/BaseMax/get-github-issues/blob/main/script.py"
      ],
      "metadata": {
        "id": "A_WxisbPO2-U"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "AiT0aTckanPl",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "7c63af2f-e701-47b6-9a15-c15a043e6c10"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "rails\n"
          ]
        }
      ],
      "source": [
        "\n",
        "# Access token created from my github account for the purpose of accessing the issues from the rails repository using pygithub\n",
        "ACCESS_TOKEN = #use your access token here,
        "g = Github(ACCESS_TOKEN)\n",
        "\n",
        "# Try block to access the repository and throw exception if there is an error\n",
        "try:\n",
        "    repo = g.get_repo(\"rails/rails\")\n",
        "    print(repo.name)\n",
        "except Exception as e:\n",
        "    print(f\"Error: {e}\")\n",
        "\n",
        "# To get all the open issues from the repository and the last 500 issues as stated in the assignment\n",
        "\n",
        "state = \"open\"\n",
        "issues = repo.get_issues(state=state, sort='created', direction = 'desc')\n",
        "\n",
        "max_issues = 500\n",
        "issue_count = 0\n",
        "\n",
        "# dictionary to hold all the issue information that go into the dataframe\n",
        "issues_data = []\n",
        "\n",
        "# Set to store unique label names\n",
        "all_labels = set()\n",
        "\n",
        "# Loop to store the issue information in the dictionary\n",
        "for issue in issues:\n",
        "    if issue_count >= max_issues:\n",
        "        break\n",
        "    if issue.pull_request is not None:\n",
        "        continue\n",
        "\n",
        "    #Retrieve comments and count the # for each issue\n",
        "    comments = issue.get_comments()\n",
        "    comment_texts = [comment.body for comment in comments]\n",
        "    #comments_str = \", \".join(comment_texts)\n",
        "    comment_count = len(comment_texts)\n",
        "\n",
        "    for label in issue.labels:\n",
        "        all_labels.add(label.name)\n",
        "\n",
        "    # Append issue data to the list\n",
        "    issue_data = {\n",
        "        \"Issue Number\": issue.number,\n",
        "        \"Title\": issue.title,\n",
        "        \"Posted By\": issue.user.login,\n",
        "        \"Created At\": issue.created_at.strftime('%B %Y'),\n",
        "        \"Updated At\": issue.updated_at.strftime('%B %Y'),\n",
        "        \"Labels\": \", \".join([label.name for label in issue.labels]) if issue.labels else \"\",\n",
        "        \"Label Count\": len(issue.labels),\n",
        "        \"Comments Count\": comment_count\n",
        "\n",
        "    }\n",
        "\n",
        "    # Add binary columns for each label (One-Hot Encoding) - for the purpose of classification\n",
        "    for label in all_labels:\n",
        "        issue_data[label] = 1 if label in [label.name for label in issue.labels] else 0\n",
        "\n",
        "    issues_data.append(issue_data)\n",
        "\n",
        "    issue_count += 1\n",
        "\n",
        "df = pd.DataFrame(issues_data)\n",
        "#print(df.head())\n",
        "\n",
        "df.to_csv(\"github_issues.csv\", index=False)\n",
        "\n",
        "label_columns = [col for col in df.columns if col not in [\"Issue Number\", \"Title\", \"Posted By\", \"Created At\", \"Updated At\", \"Labels\", \"Label Count\"]]\n",
        "df[label_columns] = df[label_columns].fillna(0)"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "# **Script to create another csv file from the original dataframe that contains the following columns for the purpose of analysis:**\n",
        "\n",
        "\n",
        "\n",
        "*   Label - All the labels from the issues\n",
        "*   Created at - The year the issue was created\n",
        "*   Cretaed By - Who created that issue\n",
        "\n"
      ],
      "metadata": {
        "id": "UGYryIbRPcou"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "labels_data = []\n",
        "\n",
        "# Process each issue in the original DataFrame\n",
        "for _, row in df.iterrows():\n",
        "    created_date = row['Created At']\n",
        "    user = row['Posted By']\n",
        "    for label in label_columns:\n",
        "        if row[label] == 1:\n",
        "            labels_data.append({\n",
        "                'Label': label,\n",
        "                'Created Date': created_date,\n",
        "                'Created By': user\n",
        "            })\n",
        "\n",
        "# Create the new DataFrame\n",
        "labels_df = pd.DataFrame(labels_data)\n",
        "\n",
        "# save the new DataFrame to a CSV\n",
        "labels_df.to_csv(\"labels_created_by_and_date.csv\", index=False)\n"
      ],
